$(function(){
	/* side-bar */
	$("#org-dashbaord").on('click', function(){
		var _popTitle = "dashboard";
		
		var _outerWidth = window.outerWidth * 0.95;
		var _outerHeight = window.outerHeight * 0.90;
		
		
		var _orgDashboardForm = document.orgDashboardForm;
		
		window.open("", _popTitle, 'height='+_outerHeight+', width='+_outerWidth + ', left=20, top=10, scrollbars=1') ;
		
		_orgDashboardForm.target = _popTitle;
		_orgDashboardForm.action = "../org/dashboard";
		_orgDashboardForm.submit();
	         
	        

	});
});