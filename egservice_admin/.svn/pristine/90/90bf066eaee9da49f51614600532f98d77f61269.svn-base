//사용량, 30일 평균 사용량, 사용량 비교, CO2 발생량 데이터를 가져온다.
function getTeamUsageDay(){
	var _today = enernet.modules.utils.getYYMMDD().split("-");
	var _yy = _today[0];
	var _mm = _today[1];
	var _dd = _today[2];
	
	//	임시데이터 
	_yy = 2020;
	_mm = 06;
	_dd = 17;
	//	임시데이터 끝
	
	
	_apiData = enernet.modules.api.getTeamUsageDay($("#seq_team").val(), _yy, _mm, _dd);
	var _get_api_time = enernet.modules.utils.getTimeStamp();
		
	
	
	//	오늘 사용량
	var _sum = _apiData.sum;
	//	30일 평균 사용량
	var _sum_avg = null;
	
	if(_apiData.is_holiday == 0){	//	근무일이면
		_sum_avg = _apiData.sum_avg_workday;
	
		
	}else{					//	휴무일이면
		_sum_avg = _apiData.sum_avg_holiday;
		_isHolidayText = "(휴무일)";
	}
		
	//	사용량 비교
	var _delta_sum = _apiData.delta_sum;
	var _delta_sum_img = null;
	if(_delta_sum < 0){
		_delta_sum = Math.abs(_delta_sum);
		_delta_sum_img = "<i class='fas fa-caret-down' style='color:blue;font-size: 22px;'></i>";  
	}else{
		_delta_sum_img = "<i class='fas fa-caret-up' style='color:red;font-size: 22px;'></i>";
	}
	
	//	CO2 발생량
	var _delta_carbon = _apiData.delta_carbon;
	var _delta_carbon_img = null;
	if(_delta_carbon < 0){
		_delta_carbon = Math.abs(_delta_carbon)
		_delta_carbon_img = "<i class='fas fa-caret-down' style='color:blue;font-size: 22px;'></i>";
	}else{
		
		_delta_carbon_img = "<i class='fas fa-caret-up' style='color:red;font-size: 22px;'></i>"; 
	}
	
	$("#sum").text(enernet.modules.utils.commaStr(_sum.toFixed(2)));			//	오늘 사용량
	$("#sum_avg").text(enernet.modules.utils.commaStr(_sum_avg.toFixed(2)));	//	30일 평균 사용량
	$("#delta_sum").text(enernet.modules.utils.commaStr(_delta_sum.toFixed(2)));	//	사용량 비교
	$("#delta_sum_img").html(_delta_sum_img);
	$("#delta_carbon").text(enernet.modules.utils.commaStr(_delta_carbon.toFixed(2)));
	$("#delta_carbon_img").html(_delta_carbon_img);
	$("#get_api_time").text(_get_api_time);
	
};


function drawChart(){
	
	// 차트 데이터 생성
	var _list_usage = _apiData.list_usage;
	
		
	var	_bar_data = [];
	var	_line_data_workday = [];
	var _line_data_holiday = [];
	var	_chart_label = [];
		
	for(var i=0; i<_list_usage.length; i++){
		_bar_data.push(_list_usage[i].usage);
		
		//	근무일이면
		_line_data_workday.push(_list_usage[i].usage_avg_workday);
		//	휴무일이면
		_line_data_holiday.push(_list_usage[i].usage_avg_holiday);
		
		
		//	라벨
		if( _list_usage[i].hhmm.indexOf(":15")>0 || _list_usage[i].hhmm.indexOf(":30")>0 || _list_usage[i].hhmm.indexOf(":45")>0 ){
			_chart_label.push("");	//	15분, 30분, 45분
			
		}else{
			if(_list_usage[i].hhmm.indexOf("00:00")>0){
				_chart_label.push("");	//	00:00 분
			}else{
				var _hhmm = _list_usage[i].hhmm.replace(":00", "");
				
				_chart_label.push(_hhmm + "시");
			}
			
		}
		
	}
	
	
	_ctx = document.getElementById('myChart');
	
	_mixedChart = new Chart(_ctx, {
	    type: 'bar',
	    data: {
	        datasets: [
	        	{
		            label: '오늘 사용량',
		            data: _bar_data,	//	bar chart
		            borderWidth: 2,
		            backgroundColor: 'rgba(255, 0, 0, 0.5)',
		            // this dataset is drawn below
		            order: 1
		        }, 
		        {
		            label: '30일 평균(근무일)',
		            data: _line_data_workday,	//	line chart
		            type: 'line',
		            borderColor: 'rgba(0, 0, 255, 0.5)',
		            fill: false,
		            order: 2
		        },
		        {
		            label: '30일 평균(휴무일)',
		            data: _line_data_holiday,	//	line chart
		            type: 'line',
		            borderColor: 'rgba(0, 180, 180, 0.5)',
		            fill: false,
		            order: 3
		        }
		        ],
	        labels: _chart_label //['January', 'February', 'March', 'April']
	    },
	    options: {
	    	scales:{
	    		xAxes: [{
	    			gridLines: {
	                    display:false
	                },
	    			ticks: {
	    				fontSize: 10
	    			}
	    		}],
	    		yAxes: [{
	    			ticks: {
	    				suggestedMax: 20,
	    				fontSize: 14
	    			},
	    			scaleLabel: {
	    				display: true,
	    				labelString: 'kWh',
	    				fontSize: 14
	    			}
	    		}]
	    	},
			responsive: true,
		    elements: {
	            point:{
	                radius: 0
	            },
	            line: {
	            	tension: 0
	            }
	        }
		}
	});
}

//	시설 목록을 가져오는 함수
function getListSite(){
	var _result = enernet.modules.api.getAdminInfo( $("#seq_admin").val());
	_list_site = _result.list_site;
	
	var _tag = null;
	
	for(var i=0; i<_list_site.length; i++){
		var _today = enernet.modules.utils.getYYMMDD().split("-");
		var _yy = _today[0];
		var _mm = _today[1];
		var _dd = _today[2];
						
		var _getSiteUsageDay = enernet.modules.api.getSiteUsageDay(_list_site[i].seq_site, _yy, _mm, _dd);
		
		_tag += "<tr class='listSiteEvt'>";
		_tag += "<input type='hidden' name='seq_site' value=" +_list_site[i].seq_site  +">" ;
		_tag += "<input type='hidden' name='site_type' value=" +_list_site[i].site_type  +">" ;
		_tag += "<input type='hidden' name='site_name' value='"+  _list_site[i].site_name +"'>" ;
		
		_tag += "<td>";
		
		if(_list_site[i].site_type == 1){	//	apt 인경우
			_tag += "<div style='background-image: url(../resources/images/red.png);background-repeat: no-repeat;background-size: contain; background-position: center'>";
			_tag += (i+1);
			_tag += "</div>";
		}else{								//	bld 인경우
			_tag +=  "<div style='background-image: url(../resources/images/yellow.png);background-repeat: no-repeat;background-size: contain; background-position: center'>";
			_tag += (i+1);
			_tag += "</div>";
		}
		
		 
		_tag += "</td>";
		
		_tag += "<td>" + _list_site[i].site_name + "</td>";
		_tag += "<td>" + enernet.modules.utils.commaStr(_getSiteUsageDay.sum.toFixed(2)) + "</td>";	//	오늘 사용량
		
		if(_getSiteUsageDay.is_holiday == 0){
			_tag += "<td>" + enernet.modules.utils.commaStr(_getSiteUsageDay.sum_avg_workday.toFixed(2)) + "</td>";	//	근무일
		}else{
			
			_tag += "<td>" + enernet.modules.utils.commaStr(_getSiteUsageDay.sum_avg_holiday.toFixed(2)) + "</td>";	//	휴무일
		}
		
		//	사용량 증감
		var _deltaSumImg = null;	
		if(_getSiteUsageDay.delta_sum>0){
			_deltaSumImg = "<i class='fas fa-arrow-up'></i>" ;
			//_deltaSum += enernet.modules.utils.commaStr(_getSiteUsageDay.delta_sum.toFixed(2))
		}else if(_getSiteUsageDay.delta_sum==0){
			
			_deltaSumImg = "";
			
		}else {
			_deltaSumImg = "<i class='fas fa-arrow-down'></i>";
			
		}
		
		_tag += "<td>" + _deltaSumImg +"&nbsp"+ enernet.modules.utils.commaStr(Math.abs(_getSiteUsageDay.delta_sum.toFixed(2))) + "</td>";
		
		//	CO2 발생량
		var _deltaCarbonImg = null;
		if(_getSiteUsageDay.delta_carbon>0){
			_deltaCarbonImg = "<i class='fas fa-arrow-up'></i>" ;
		}else if(_getSiteUsageDay.delta_carbon==0){
			_deltaCarbonImg = "";
		}else{
			_deltaCarbonImg = "<i class='fas fa-arrow-down'></i>";
			
		}
		
		_tag += "<td>" + _deltaCarbonImg +"&nbsp"+ enernet.modules.utils.commaStr(Math.abs(_getSiteUsageDay.delta_carbon.toFixed(2))) + "</td>";
		
		_tag += "</tr>";
		
	}
	
	$("#listSiteTable").append(_tag);
	
	
	//	클릭 이벤트
	var _listSiteEvt = document.getElementsByClassName('listSiteEvt')
	
	for(var i=0; i<_listSiteEvt.length; i++){
		_listSiteEvt[i].onclick = function(e){
			var _seqSiteInput = e.currentTarget.children["seq_site"];
			var _siteTypeInput = e.currentTarget.children["site_type"];
			var _siteName = e.currentTarget.children["site_name"];
			var _site_type = _siteTypeInput.value;
			
			createForm(_site_type);
			document.popForm.append(_seqSiteInput.cloneNode())
			document.popForm.append(_siteTypeInput.cloneNode())
			document.popForm.append(_siteName.cloneNode())
			
			sitePopup();
			
		}
	};
	
}//


function initMap(){
//	맵 실행
	var _center = {lat: 37.464824, lng: 126.90064};
	_googleMap = new google.maps.Map(document.getElementById('siteListMap'),
	                                  		{	zoom: 14,
				                                center: _center,
				                                zoomControl: true,
				                                mapTypeControl: false,
				                                streetViewControl: false,
				                                rotateControl: false,
				                                fullscreenControl: false
	                                  		}
	    								);
	
	
	var _centerControlDiv = document.createElement('div');
	var _centerControl = new CenterControl(_centerControlDiv, _googleMap, _center);

	_centerControlDiv.index = 1;
	_googleMap.controls[google.maps.ControlPosition.BOTTOM_LEFT].push(_centerControlDiv);

	//	폴리 라인을 그림
	var _geumcheonPoly = null;
	var _geumcheonCoord = [];
	for (var i =0;i<_lst.length;i++){
		var x = _lst[i][1] + 0.00273;
		var y = _lst[i][0] - 0.002;
		_geumcheonCoord.push(new google.maps.LatLng(x,y));
	}

	_geumcheonPoly = new google.maps.Polygon({
	    paths: _geumcheonCoord,
	    strokeColor: "#FF0000",
	    strokeOpacity: 0.8,
	    strokeWeight: 2,
	    fillOpacity:0
	  });

	//	폴리라인 + 맵 생성
	_geumcheonPoly.setMap(_googleMap);
	
	
	//	마커	이미지
	for (var i = 0; i < _list_site.length; i++) {
		var _markerImg = null;
		
		if(_list_site[i].site_type == '1'){		//	1 == 아파트
			_markerImg = "../resources/images/red.png";
		}else{									//	2 == 건물
			_markerImg = "../resources/images/yellow.png";
		}
		
		_marker = new google.maps.Marker({
	          position : new google.maps.LatLng(_list_site[i].site_dy, _list_site[i].site_dx),		//	위도경도
	          title : _list_site[i].site_name,			//	건물,단지명
	          icon: _markerImg,	
	          label : (i+1).toString(),
	          siteData : _list_site[i],		//	커스텀 데이터 
	          map: _googleMap
	        });
		
		// 클릭 이벤트 연결
		_marker.addListener('click', onClickMarker);
	}
	 
}


//	센터 이벤트 
function CenterControl(controlDiv, map,center) {

	  // Set CSS for the control border.
	  var controlUI = document.createElement('div');
	  controlUI.style.backgroundColor = '#fff';
	  controlUI.style.border = '2px solid #fff';
	  controlUI.style.borderRadius = '3px';
	  controlUI.style.boxShadow = '0 2px 6px rgba(0,0,0,.3)';
	  controlUI.style.cursor = 'pointer';
	  controlUI.style.marginBottom = '22px';
	  controlUI.style.textAlign = 'center';
	  controlUI.title = 'Click to recenter the map';
	  controlDiv.appendChild(controlUI);

	  // Set CSS for the control interior.
	  var controlText = document.createElement('div');
	  controlText.style.color = 'rgb(25,25,25)';
	  controlText.style.fontFamily = 'Roboto,Arial,sans-serif';
	  controlText.style.fontSize = '16px';
	  controlText.style.lineHeight = '38px';
	  controlText.style.paddingLeft = '5px';
	  controlText.style.paddingRight = '5px';
	  controlText.innerHTML = 'Center Map';
	  controlUI.appendChild(controlText);

	  // Setup the click event listeners: simply set the map to Chicago.
	  controlUI.addEventListener('click', function() {
		  map.setCenter(center);
		  map.setZoom(14);
	  });

}


//마커 클릭이벤트
function onClickMarker(){
	//console.log(this.title);
	
	
	
	createForm(this.siteData.site_type);
	
		
	
	var _siteData = this.siteData;
	var _keys = Object.keys(_siteData);
	
	var _popForm = document.popForm;
	
	
	for(var i=0; i<_keys.length; i++){
			
		var _hiddenField = document.createElement("input"); 
		_hiddenField.setAttribute("type", "hidden"); 
		_hiddenField.setAttribute("name", _keys[i]); 
		_hiddenField.setAttribute("value", _siteData[_keys[i]]); 
		_popForm.appendChild(_hiddenField); 
		
	}
	
	
	sitePopup();	
}


function createForm(siteType){
	
	console.log(siteType);
	
	if(document.getElementsByName('popForm').length != 0){
		$("#popForm").remove();
	}
	
	var form = document.createElement("form"); 
	form.setAttribute("id", "popForm") 
	form.setAttribute("name", "popForm") 
	form.setAttribute("charset", "UTF-8"); 
	form.setAttribute("method", "post");   
	
	form.setAttribute("action", "../org/pop/bldProc"); 
	document.body.appendChild(form); 
}


function sitePopup(){
	var _popForm = document.popForm;
	
	var _outerWidth = window.outerWidth * 0.95;
	var _outerHeight = window.outerHeight * 0.90;
	
	window.open("", 'popForm', 'height='+_outerHeight+', width='+_outerWidth + ', left=20, top=10');
	_popForm.target = 'popForm';
	_popForm.submit();
}



