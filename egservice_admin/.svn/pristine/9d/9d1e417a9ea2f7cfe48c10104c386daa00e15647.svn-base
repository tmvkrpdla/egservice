body {
	margin: 0;
	padding: 0;
	
}
/* .div {
	border: 1px solid red;
} */

/* .printPage {
	border: 1px solid red;
}  */

* {
	box-sizing: border-box;
	-moz-box-sizing: border-box;
}

.printPage {
	width: 21cm;
	margin: auto;

	padding-top: 30px;
	padding-bottom: 30px;

	padding-left: 25px;
	padding-right: 25px;;
	border: 1px solid black;
}

@page {
	size: A4;
	margin: 0;
}

@media print {
	html, body {
		width: 210mm;
		
	}
	.printPage {
		padding-left: 25px;
		padding-right: 25px;;
		padding-top:20px;
		padding-bottom : 20px;
		border: initial;
		width: initial;
		min-height: initial;
		box-shadow: initial;
		background: initial;
	}
}
