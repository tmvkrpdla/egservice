#menu {
	display: flex;
	height: 42px;
	background: #B4B5B6;
	width: 90%;
	margin: 0 auto;
	font-size: 16px;
	justify-content: flex-end;
}

#mn_list {
	list-style: none;
	display: flex;
	flex-direction: row;
	margin: 0;
	width: 100%;
	display: flex;
	justify-content: center;
}

#menu ul li {
	list-style: none;
	float: left;
	padding: 0px 10px;
	height: 100%;
	text-align: center;
	text-decoration-color: black;
	width: 10%;
	line-height: 40px;
}

#menu ul ul {
	display: none;
	width: 300px;
	height: 90%;
	text-align: center;
	justify-content: center;
}

#mn_list li:hover>ul {
	display: flex;
	justify-content: center;
}

#mn_list li li {
	float: left;
	list-style-type: none;
	width: 100px;
	height: 60%;
	line-height: 40px;
	font-weight: 500;
	font-size: 6pt;
}

#mn_list .mn_type1 {
	background: #F4C221;
	border-radius: 8px;
}

#mn_list .mn_type2 {
	background: #B4B5B6;
	border-radius: 8px;
}

li a {
	color: black;
	font-weight: 900;
	font-size: 8pt;
}

.lnb_list li a {
	font-size: 15px;
}

